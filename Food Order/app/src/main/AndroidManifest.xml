<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.edu.itl.c18131250.foodorder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_my_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodOrder">
        <activity android:name=".CarritoActivity"></activity>
        <activity android:name=".AcercaDeActivity" />
        <activity android:name=".PagoActivity" />
        <activity android:name=".BebidasActivity" />
        <activity android:name=".CenaActivity" />
        <activity android:name=".ComidaActivity" />
        <activity android:name=".AlmuerzoActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>